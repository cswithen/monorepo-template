{
  "plugins": ["stylelint-prettier"],
  "extends": ["stylelint-config-sass-guidelines", "stylelint-prettier/recommended"],
  "rules": {
    "scss/at-import-no-partial-leading-underscore": null,
    "selector-class-pattern": null,
    "max-nesting-depth": 2
  },
  "ignoreFiles": [
    "src/base/reset/*.scss"
  ]
}